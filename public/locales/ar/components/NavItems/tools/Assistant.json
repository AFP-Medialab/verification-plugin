 "thumbnails_text": "\u0627\u0644\u0628\u062d\u062b \u0627\u0644\u0639\u0643\u0633\u064a \u0639\u0644\u0649 \u0627\u0644\u0635\u0648\u0631 \u0627\u0644\u0645\u0635\u063a\u0631\u0629 \u0645\u0646 \u0627\u0644\u0641\u064a\u062f\u064a\u0648", "magnifier_text": "\u0641\u062d\u0635 \u0645\u062a\u0639\u0645\u0642 \u0644\u0644\u0635\u0648\u0631\u0629 (\u0639\u062f\u0633\u0629 \u0645\u0643\u0628\u0631\u0629)", "metadata_text": "\u0627\u0633\u062a\u0631\u062f\u0627\u062f \u0628\u064a\u0627\u0646\u0627\u062a \u062a\u0639\u0631\u064a\u0641 \u0627\u0644\u0635\u0648\u0631\u0629/\u0627\u0644\u0641\u064a\u062f\u064a\u0648", "rights_text": "\u0627\u0633\u062a\u0631\u062f\u0627\u062f \u0627\u0644\u0645\u0639\u0644\u0648\u0645\u0627\u062a \u0627\u0644\u0645\u062a\u0639\u0644\u0642\u0629 \u0628\u062d\u0642\u0648\u0642 \u0627\u0644\u0641\u064a\u062f\u064a\u0648 \u0641\u0642\u0637", "forensic_text": "\u0643\u0634\u0641 \u0623\u064a \u062a\u0644\u0627\u0639\u0628 \u0628\u0627\u0644\u0635\u0648\u0631", "ocr_text": "\u0627\u0633\u062a\u062e\u0631\u0627\u062c \u0623\u064a \u0646\u0635 \u0645\u0646 \u0627\u0644\u0635\u0648\u0631\u0629", "hyperpartisan_title": "\u062e\u062f\u0645\u0629 \u0627\u0644\u0643\u0634\u0641 \u0639\u0646 \u0627\u0644\u062a\u0634\u062f\u062f \u0627\u0644\u062d\u0632\u0628\u064a (Hyperpartisan)", "dbkf_text_title": "\u0642\u0627\u0639\u062f\u0629 \u0628\u064a\u0627\u0646\u0627\u062a \u0627\u0644\u0628\u062d\u062b \u0639\u0646 \u0627\u0644\u0646\u0635\u0648\u0635 \u0627\u0644\u0645\u0632\u064a\u0641\u0629 \u0627\u0644\u0645\u0639\u0631\u0648\u0641\u0629", "dbkf_media_title": "\u0642\u0627\u0639\u062f\u0629 \u0628\u064a\u0627\u0646\u0627\u062a \u0627\u0644\u0628\u062d\u062b \u0639\u0646 \u0627\u0644\u0648\u0633\u0627\u0626\u0637 \u0627\u0644\u0645\u0632\u064a\u0641\u0629 \u0627\u0644\u0645\u0639\u0631\u0648\u0641\u0629", "ne_title": "\u0627\u0644\u0628\u062d\u062b \u0639\u0646 \u0643\u064a\u0627\u0646 \u0645\u0633\u0645\u0649", "ocr_title": "\u0628\u062d\u062b OCR", "source_cred_title": "\u062e\u062f\u0645\u0629 \u0645\u0635\u062f\u0627\u0642\u064a\u0629 \u0627\u0644\u0645\u0635\u062f\u0631", "mt_title": "\u062e\u062f\u0645\u0629 \u0627\u0644\u062a\u0631\u062c\u0645\u0629 \u0627\u0644\u0622\u0644\u064a\u0629", "translate": "\u062a\u0631\u062c\u0645\u0629 (\u0628\u064a\u062a\u0627)", "copy_link": "\u0627\u0646\u0633\u062e \u0627\u0644\u0631\u0627\u0628\u0637", "archive_link": "\u0627\u0644\u0623\u0631\u0634\u064a\u0641", "source_credibility_warning": "\u062a\u0645 \u0630\u0643\u0631 \u0627\u0644\u0646\u0637\u0627\u0642 \u0623\u0648 \u0627\u0644\u062d\u0633\u0627\u0628 \u0641\u064a \u0639\u0645\u0644\u064a\u0629 \u0628\u062d\u062b \u0636\u062f:", "text_tooltip": "<b> \u0645\u0627 \u0647\u0630\u0627\u061f</b></br> \u064a\u062a\u0645 \u0639\u0631\u0636 \u0623\u064a \u0646\u0635 \u064a\u0645\u0643\u0646 \u0627\u0633\u062a\u062e\u0631\u0627\u062c\u0647 \u0645\u0646 \u0639\u0646\u0648\u0627\u0646 URL \u0627\u0644\u0645\u062d\u062f\u062f \u0647\u0646\u0627</br></br><b>\u0643\u064a\u0641 \u064a\u062a\u0645 \u0627\u0633\u062a\u062e\u062f\u0627\u0645 \u0647\u0630\u0627\u061f</b></br> \u064a\u062a\u0645 \u062a\u0634\u063a\u064a\u0644 \u0623\u062f\u0648\u0627\u062a \u0645\u0639\u0627\u0644\u062c\u0629 \u0627\u0644\u0646\u0635\u0648\u0635 \u0627\u0644\u0645\u062e\u062a\u0644\u0641\u0629 \u0645\u0642\u0627\u0628\u0644 \u0647\u0630\u0627 \u0627\u0644\u0646\u0635: \u064a\u062a\u0645 <ul><li>\u0627\u0643\u062a\u0634\u0627\u0641 \u0627\u0644\u0644\u063a\u0629</li> \u0648\u0639\u0631\u0636\u0647\u0627 <li>\u064a\u062a\u0645 \u0627\u0633\u062a\u062e\u0631\u0627\u062c \u0627\u0644\u0643\u064a\u0627\u0646\u0627\u062a \u0627\u0644\u0645\u0633\u0645\u0627\u0629 \u0641\u064a \u0627\u0644\u0646\u0635 \u0648\u0639\u0631\u0636\u0647\u0627 (\u0627\u0644\u0623\u0634\u062e\u0627\u0635 \u0648\u0627\u0644\u0645\u0648\u0627\u0642\u0639 \u0648\u0627\u0644\u0645\u0646\u0638\u0645\u0627\u062a \u0648\u0645\u0627 \u0625\u0644\u0649 \u0630\u0644\u0643)</li> <li>\u064a\u062a\u0645 \u0625\u0631\u0633\u0627\u0644 \u0623\u0648\u0644 500 \u062d\u0631\u0641 \u0641\u064a \u0627\u0644\u0646\u0635 \u0644\u064a\u062a\u0645 \u0627\u0644\u062a\u062d\u0642\u0642 \u0645\u0646\u0647\u0627 \u0645\u0642\u0627\u0628\u0644 \u0642\u0627\u0639\u062f\u0629 \u0628\u064a\u0627\u0646\u0627\u062a \u0627\u0644\u0645\u0646\u062a\u062c\u0627\u062a \u0627\u0644\u0645\u0632\u064a\u0641\u0629 \u0627\u0644\u0645\u0639\u0631\u0648\u0641\u0629. \u064a\u062a\u0645 \u0633\u0631\u062f \u0646\u062a\u0627\u0626\u062c \u0647\u0630\u0627 \u0627\u0644\u0641\u062d\u0635 (\u0625\u0646 \u0648\u062c\u062f\u062a) \u0641\u064a \u0642\u0633\u0645 <b>\u0627\u0644\u062a\u062d\u0630\u064a\u0631\u0627\u062a</b> \u0644\u0645\u0632\u064a\u062f \u0645\u0646</li></ul> \u0627\u0644\u0645\u0639\u0644\u0648\u0645\u0627\u062a\u060c \u0631\u0627\u062c\u0639: <a href=\"https://gatenlp.github.io/we-verify-app-assistant/supported-tools\" target=\"_blank\" rel=\"noopener noreferrer\">this page</a>", "media_tooltip": "<b> \u0645\u0627 \u0647\u0630\u0627\u061f</b></br> \u062a\u0639\u0631\u0636 \u0623\u064a \u0635\u0648\u0631 \u0623\u0648 \u0645\u0642\u0627\u0637\u0639 \u0641\u064a\u062f\u064a\u0648 \u064a\u0645\u0643\u0646 \u0627\u0633\u062a\u062e\u0631\u0627\u062c\u0647\u0627 \u0645\u0646 \u0639\u0646\u0648\u0627\u0646 URL \u0647\u0646\u0627.</br></br> <b>\u0645\u0627\u0630\u0627 \u064a\u0645\u0643\u0646\u0646\u064a \u0623\u0646 \u0623\u0641\u0639\u0644 \u0628\u0647\u0627\u061f</b></br> \u0628\u0627\u0644\u0646\u0633\u0628\u0629 \u0644\u0623\u064a \u0635\u0648\u0631\u0629 / \u0641\u064a\u062f\u064a\u0648 \u0645\u062d\u062f\u062f\u060c \u064a\u062a\u0645 \u062a\u0642\u062f\u064a\u0645 \u0642\u0627\u0626\u0645\u0629 \u0628\u0627\u0644\u0623\u062f\u0648\u0627\u062a \u0630\u0627\u062a \u0627\u0644\u0635\u0644\u0629 \u0645\u0646 \u0627\u0644\u0645\u0643\u0648\u0646 \u0627\u0644\u0625\u0636\u0627\u0641\u064a \u0648\u0627\u0644\u062a\u064a \u064a\u0645\u0643\u0646\u0647\u0627 \u0645\u0639\u0627\u0644\u062c\u0629 \u0647\u0630\u0647 \u0627\u0644\u0648\u0633\u0627\u0626\u0637. \u0633\u064a\u0624\u062f\u064a \u062a\u062d\u062f\u064a\u062f \u0623\u064a \u0645\u0646 \u0647\u0630\u0647 \u0627\u0644\u0648\u0633\u0627\u0626\u0637 \u0625\u0644\u0649 \u0625\u0639\u0627\u062f\u0629 \u062a\u0648\u062c\u064a\u0647 \u0627\u0644\u0648\u0633\u0627\u0626\u0637 \u0625\u0644\u0649 \u0627\u0644\u0623\u062f\u0627\u0629 \u0627\u0644\u0645\u062d\u062f\u062f\u0629.</br> \u0625\u0644\u0649 \u062c\u0627\u0646\u0628 \u0630\u0644\u0643\u060c \u0633\u064a\u0642\u0648\u0645 \u0627\u0644\u0645\u0633\u0627\u0639\u062f \u0628\u0639\u0643\u0633 \u0627\u0644\u0628\u062d\u062b \u0641\u064a \u0627\u0644\u0648\u0633\u0627\u0626\u0637 \u0645\u0642\u0627\u0628\u0644 \u0642\u0627\u0639\u062f\u0629 \u0628\u064a\u0627\u0646\u0627\u062a \u0627\u0644\u0645\u0646\u062a\u062c\u0627\u062a \u0627\u0644\u0645\u0632\u064a\u0641\u0629 \u0627\u0644\u0645\u0639\u0631\u0648\u0641\u0629. \u0625\u0630\u0627 \u062a\u0645 \u0627\u0644\u0639\u062b\u0648\u0631 \u0639\u0644\u0649 \u0623\u064a \u0646\u062a\u0627\u0626\u062c\u060c \u0633\u064a\u062a\u0645 \u0639\u0631\u0636\u0647\u0627 \u0641\u064a \u0642\u0633\u0645 \u0627\u0644\u062a\u062d\u0630\u064a\u0631\u0627\u062a. <b></b></br></br> \u0644\u0645\u0632\u064a\u062f \u0645\u0646 \u0627\u0644\u0645\u0639\u0644\u0648\u0645\u0627\u062a\u060c \u0631\u0627\u062c\u0639 \u0647\u0630\u0647 \u0627\u0644\u0635\u0641\u062d\u0629: <a href=\"https://gatenlp.github.io/we-verify-app-assistant/supported-tools\" target=\"_blank\" rel=\"noopener noreferrer\"></a>", "sc_tooltip": "<b>\u0645\u0627 \u0647\u0630\u0627\u061f</b><br> \u0644\u0643\u0644 \u0639\u0646\u0648\u0627\u0646 URL \u064a\u062a\u0645 \u0625\u062f\u062e\u0627\u0644\u0647\u060c \u0646\u0642\u0648\u0645 \u0628\u062a\u0634\u063a\u064a\u0644 \u0627\u0644\u0646\u0637\u0627\u0642 (\u0625\u0630\u0627 \u0643\u0627\u0646 \u0647\u0630\u0627 \u0639\u0646\u0648\u0627\u0646 URL \u063a\u064a\u0631 \u0645\u062a\u0639\u0644\u0642 \u0628\u0648\u0633\u0627\u0626\u0637 \u0627\u0644\u062a\u0648\u0627\u0635\u0644 \u0627\u0644\u0627\u062c\u062a\u0645\u0627\u0639\u064a) \u0623\u0648 \u0627\u0644\u062d\u0633\u0627\u0628 (\u0625\u0630\u0627 \u0643\u0627\u0646 \u0647\u0630\u0627 \u0639\u0646\u0648\u0627\u0646 URL \u0645\u062a\u0639\u0644\u0642 \u0628\u0648\u0633\u0627\u0626\u0637 \u0627\u0644\u062a\u0648\u0627\u0635\u0644 \u0627\u0644\u0627\u062c\u062a\u0645\u0627\u0639\u064a) \u0639\u0644\u0649 \u062e\u062f\u0645\u0629 \u062a\u062d\u0644\u064a\u0644 \u0627\u0644\u0646\u0637\u0627\u0642 / \u0627\u0644\u062d\u0633\u0627\u0628 URL. \u0648\u062a\u0638\u0647\u0631 \u0646\u062a\u0627\u0626\u062c \u0647\u0630\u0627 \u0627\u0644\u0641\u062d\u0635 \u0647\u0646\u0627.</br></br>\u0645\u0627 \u0627\u0644\u0630\u064a \u064a\u0645\u0643\u0646\u0646\u064a \u0641\u0639\u0644\u0647 \u0628\u0647\u0630\u0647 \u0627\u0644\u0646\u062a\u0627\u0626\u062c\u061f</b><br> \u062a\u0647\u062f\u0641 \u062e\u062f\u0645\u0629 \u062a\u062d\u0644\u064a\u0644 \u0627\u0644\u0646\u0637\u0627\u0642 / \u0627\u0644\u062d\u0633\u0627\u0628 URL \u0625\u0644\u0649 \u062c\u0645\u0639 \u0627\u0644\u0645\u0639\u0644\u0648\u0645\u0627\u062a \u062d\u0648\u0644 \u0646\u0637\u0627\u0642 / \u062d\u0633\u0627\u0628 \u0645\u0646 \u0645\u0635\u0627\u062f\u0631 \u0645\u062a\u0639\u062f\u062f\u0629. \u0648\u0646\u0639\u062a\u0632\u0645 \u0645\u0646 \u062e\u0644\u0627\u0644\u0647\u0627 \u0625\u0639\u0644\u0627\u0645 \u0627\u0644\u0645\u0633\u062a\u062e\u062f\u0645 \u0645\u0627 \u0625\u0630\u0627 \u0643\u0627\u0646\u062a \u0623\u064a\u064b\u0627 \u0645\u0646 \u0627\u0644\u0645\u0635\u0627\u062f\u0631 \u0627\u0644\u062a\u064a \u0646\u062c\u0645\u0639 \u0645\u0646\u0647\u0627 \u062a\u062d\u062a\u0648\u064a \u0639\u0644\u0649 \u0645\u0639\u0644\u0648\u0645\u0627\u062a \u062d\u0648\u0644 \u0627\u0644\u0646\u0637\u0627\u0642 / \u0627\u0644\u062d\u0633\u0627\u0628 \u0644\u0639\u0646\u0648\u0627\u0646 URL \u0627\u0644\u0645\u062f\u062e\u0644. \u0648\u064a\u062a\u0645 \u062a\u0642\u0633\u064a\u0645 \u0627\u0644\u0646\u062a\u064a\u062c\u0629 \u0625\u0644\u0649 \u062b\u0644\u0627\u062b\u0629 \u0623\u0646\u0648\u0627\u0639:<ul> <li> <strong> \u062a\u062d\u0630\u064a\u0631: </ strong> \u0627\u0644\u0646\u0637\u0627\u0642 \u0645\u064f\u062f\u0631\u062c \u062d\u0633\u0628 \u0627\u0644\u0645\u0635\u062f\u0631 \u0648\u0627\u0644\u0630\u064a \u064a\u0633\u0631\u062f \u0645\u0648\u062b\u0648\u0642\u064a\u0629 \u0627\u0644\u0646\u0637\u0627\u0642\u0627\u062a \u0627\u0644\u062a\u064a \u064a\u064f\u062d\u062a\u0645\u0644 \u0623\u0646 \u062a\u0643\u0648\u0646 \u063a\u064a\u0631 \u062c\u062f\u064a\u0631\u0629 \u0628\u0627\u0644\u062b\u0642\u0629 </ li> <li> <strong> \u0645\u062f\u0642\u0642 \u0627\u0644\u0645\u0639\u0644\u0648\u0645\u0627\u062a: </ strong> \u0627\u0644\u0646\u0637\u0627\u0642 \u0645\u064f\u062f\u0631\u062c \u062d\u0633\u0628 \u0627\u0644\u0645\u0635\u062f\u0631 \u0648\u0627\u0644\u0630\u064a \u064a\u0633\u0631\u062f \u0628\u0634\u0643\u0644 \u0635\u0631\u064a\u062d \u0645\u062f\u0642\u0642\u064a \u0627\u0644\u0645\u0639\u0644\u0648\u0645\u0627\u062a </ li> <li> <strong> \u0627\u0644\u0625\u0634\u0627\u0631\u0627\u062a: </ strong> \u0627\u0644\u0646\u0637\u0627\u0642 \u0645\u064f\u062f\u0631\u062c \u062d\u0633\u0628 \u0627\u0644\u0645\u0635\u062f\u0631 \u0648\u0627\u0644\u0630\u064a \u064a\u0633\u0631\u062f \u0645\u0632\u064a\u062c\u064b\u0627 \u0645\u0646 \u0627\u0644\u0646\u0637\u0627\u0642\u0627\u062a \u0627\u0644\u0645\u0648\u062b\u0648\u0642\u0629 \u0648\u063a\u064a\u0631 \u0627\u0644\u062c\u062f\u064a\u0631\u0629 \u0628\u0627\u0644\u062b\u0642\u0629 </ li> </ul> \u0648\u0646\u0638\u0631\u064b\u0627 \u0644\u0623\u0646 \u0647\u0630\u0647 \u0627\u0644\u0645\u0639\u0644\u0648\u0645\u0627\u062a \u062c\u0645\u0639\u062a \u0645\u0646 \u0645\u0635\u0627\u062f\u0631 \u0645\u062a\u0639\u062f\u062f\u0629\u060c \u0641\u0642\u062f \u0642\u0645\u0646\u0627 \u0628\u0625\u0639\u0637\u0627\u0621 \u0627\u0644\u0645\u0635\u062f\u0631 \u0627\u0644\u0630\