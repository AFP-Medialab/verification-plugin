{
  "tabWidth": 2,
  "useTabs": false,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "react",
    "@mui/material",
    "@mui/material/(.*)",
    "@mui/icons-material",
    "@mui-icons",
    "<THIRD_PARTY_MODULES>",
    "^[./]",
    "^[.]/[^./]",
    "^[.]/[^./]/(.*)$",
    "^[.]/",
    "<INTERNAL_MODULES>"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
