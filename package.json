  "scripts": {
    "dev": "wxt --mode development",
    "dev:chrome": "wxt -b chrome --mode development",
    "dev:firefox": "wxt -b firefox --mode development",
    "build": "wxt build --mode production",
    "build:chrome:development": "wxt build -b chrome --mode development",
    "build:chrome:production": "wxt build -b chrome --mode production",
    "build:firefox:development": "wxt build -b firefox --mode development",
    "build:firefox:production": "wxt build -b firefox --mode production",
    "build:all:development": "wxt build -b chrome --mode development && wxt build -b firefox --mode development",
    "build:all:production": "wxt build -b chrome --mode production && wxt build -b firefox --mode production",
    "prepare": "husky",
    "postinstall": "wxt prepare",
    "zip": "wxt zip",
    "zip:chrome:development": "wxt zip -b chrome --mode development",
    "zip:chrome:production": "wxt zip -b chrome --mode production",
    "zip:firefox:development": "wxt zip -b firefox --mode development",
    "zip:firefox:production": "wxt zip -b firefox --mode production",
    "zip:all:development": "wxt zip -b chrome --mode development && wxt zip -b firefox --mode development",
    "zip:all:production": "wxt zip -b chrome --mode production && wxt zip -b firefox --mode production",
    "test": "npm run test-cu && npm run test-e2e",
    "test-cu": "rm -rf playwright/.cache && playwright test -c playwright-ct.config.js",
    "test-e2e": "playwright test",
    "test-e2e-ui": "playwright test --ui"
  },
